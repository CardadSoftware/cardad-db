{
  "name": "cardad-db",
  "version": "1.0.0",
  "description": "Database Schema for cardad",
  "types": "dist/types/cardadSchema.d.ts",
  "exports":{
    "./interfaces": {
      "types": "./dist/types/interfaces/index.d.ts",
      "default": "./dist/interfaces/index.js"
    },
    "./interfaces/*": {
      "types": "./dist/types/interfaces/*.d.ts",
      "default": "./dist/interfaces/*.js"
    },
    "./models": {
      "types": "./dist/types/models/index.d.ts",
      "default": "./dist/models/index.js"
    },
    "./models/*": {
      "types": "./dist/types/models/*.d.ts",
      "default": "./dist/models/*.js"
    },
    "./schemas": {
      "types": "./dist/types/schemas/index.d.ts",
      "default": "./dist/schemas/index.js"
    },
    "./schemas/*": {
      "types": "./dist/types/schemas/*.d.ts",
      "default": "./dist/schemas/*.js"
    }
  },
  "scripts": {
    "build": "npx tsc",
    "clean": "rimraf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/crackpotmgee/cardad-db.git"
  },
  "author": "Anthony Stoute",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/crackpotmgee/cardad-db/issues"
  },
  "homepage": "https://github.com/crackpotmgee/cardad-db#readme",

  "dependencies": {
    "esm": "^3.2.25",
    "mongoose": "^6.13.5",
    "passport-local-mongoose": "^7.1.2"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "typescript": "^4.9.5",
    "rimraf": "^5.0.5"
  }
}
