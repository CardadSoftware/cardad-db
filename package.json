{
  "name": "cardad-db",
  "version": "1.0.0",
  "description": "Database Schema for cardad",
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/index.js"
    },
    "./interfaces": {
      "types": "./dist/types/interfaces/index.d.ts",
      "default": "./dist/interfaces/index.js"
    },
    "./interfaces/*": {
      "types": "./dist/types/interfaces/*.d.ts",
      "default": "./dist/interfaces/*.js"
    },
    "./models": {
      "types": "./dist/types/models/index.d.ts",
      "default": "./dist/models/index.js"
    },
    "./schemas": {
      "types": "./dist/types/schemas/index.d.ts",
      "default": "./dist/schemas/index.js"
    },
    "./utilities":{
      "types": "./dist/types/utilities/index.d.ts",
      "default": "./dist/utilities/index.js"
    },
    "./settings/schemas":{
      "types": "./dist/types/schemas/settings/index.d.ts",
      "default": "./dist/schemas/settings/index.js"
    },
    "./settings/models":{
      "types": "./dist/types/models/settings/index.d.ts",
      "default": "./dist/schemas/models/index.js"
    }
  },
  "scripts": {
    "build": "npx tsc",
    "clean": "rimraf dist/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/crackpotmgee/cardad-db.git"
  },
  "author": "Anthony Stoute",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/crackpotmgee/cardad-db/issues"
  },
  "homepage": "https://github.com/crackpotmgee/cardad-db#readme",
  "dependencies": {
    "esm": "^3.2.25",
    "mongoose": "^6.13.8",
    "passport-local-mongoose": "^7.1.2"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "rimraf": "^5.0.5",
    "typescript": "^4.9.5"
  }
}
